#include <stdio.h>
#include<math.h>
#include <stdlib.h>
int main() {
  int n;
  scanf("%d",&n);
  for(int i=0;i<n;i++){
    int a;
    scanf("%d",&a);
  int count=0;
  int ans=1;
  
  while(a>0){
    if(a%2==1){
        count++;
    }
    a=a/2;
  }
  for(int j=0;j<count;j++){
    ans=ans*2;
  }
  ans=ans-1;
  printf("%d\n",ans);
}
}
